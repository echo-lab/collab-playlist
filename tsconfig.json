{
  "compilerOptions": {
    "target": "ES2019",
    "lib": [
      // "dom",
      // "dom.iterable",
      // "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    // "strict": true,
    "forceConsistentCasingInFileNames": true,
    // "module": "esnext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    // "noEmit": true,
    "noEmit": false,
    "strict": false,
    // "noImplicitAny": false,
    "outDir": "build",
    "sourceMap": true,
  },
  "include": [
    "src",
    "client/src/shared/"
  ],
  "exclude": [
    "client",
    "build"
  ],
  "types": [
    "express"
  ]
}
