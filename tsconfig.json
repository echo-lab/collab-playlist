{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      // "dom",
      // "dom.iterable",
      // "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    // "strict": true,
    "forceConsistentCasingInFileNames": true,
    // "module": "esnext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
    "noEmit": false,
    "strict": false,
    // "noImplicitAny": false,
    "outDir": "build",
    "watch": true,
    "preserveWatchOutput": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "client",
    "build"
  ]
}
